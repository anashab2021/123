pipeline {
    agent any
    stages {
        stage('Install the git') {
            steps {
        	customWorkspace '/home/ubuntu/123'        
		sshagent(['ssh']) {
		sh 'git pull https://github.com/anashab2021/123.git'
		sh 'docker-compose -f /home/ubuntu/123/docker-compose.yml up -d'

            }
        }
    }
        stage('Run docker compose') {
            steps {
                sshagent(['ssh']) {
		sh 'docker-compose -f /home/ubuntu/123/docker-compose.yml up -d'
            }
        }
	}
    }
}
