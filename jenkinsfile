pipeline {
#    agent any 
    sshagent(['ssh']) {
    
	stages {
        stage('Install the git') {
            steps {
                sh 'cd /home/ubuntu/123 && git pull https://github.com/anashab2021/123.git'
            }
        }
        stage('Run docker compose') {
            steps {
                sh 'docker-compose -f /home/ubuntu/123/docker-compose.yml up -d --build'
            }
        }
    }
}
}
