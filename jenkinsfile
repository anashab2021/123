pipeline {
    agent any
    stages {
        stage('Install the git') {
            steps {
                sh 'ssh -i ubuntu@172.31.38.14 "cd /home/ubuntu/123 && git pull https://github.com/anashab2021/123.git"'
            }
        }
        stage('Run docker compose') {
            steps {
                sh 'ssh -i ubuntu@172.31.38.14 "docker-compose -f /home/ubuntu/123/docker-compose.yml up -d --build"'
            }
        }
    }
}
